			</Enum>
	
			<Enum name="RECURRENCE_SUPPORT_FIELD_DOMESTICNONE">
				<Element name="ExpiryDate" type="Date"/>
			</Enum>
			
			
			<Enum name="RECURRENCE_SUPPORT_FIELD_DOMESTICWEEKLY">
				<Element name="DayOfWeek" type="int"/>
