				<Element Name="path" value="3"/>
				<Element Name="object" value="4"/>
			</Enum>
	   
			<Enum Name="DONE_STATE">
				<Element Name="Done" value="0"/>
				<Element Name="Undone" value="1"/>
			</Enum>
	
			<Enum name="RECURRENCE_TYPE_DOMESTIC_A">
				<Element name="None" type="RECURRENCE_SUPPORT_FIELD_DOMESTICNONE"/>
