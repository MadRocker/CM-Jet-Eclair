				<Element name="Yearly"  type="RECURRENCE_SUPPORT_FIELD_DOMESTICYEARLY"/>
			</Enum>
	
			<Enum name="RECURRENCE_TYPE_APPOINTMENT_A">
				<Element name="None" type="RECURRENCE_SUPPORT_FIELD_DOMESTICNONE"/>
