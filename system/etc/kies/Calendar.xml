				<Element name="Weekly" type="RECURRENCE_SUPPORT_FIELD_DOMESTICNONE" />
				<Element name="Monthly" type="RECURRENCE_SUPPORT_FIELD_DOMESTICNONE" />
			</Enum>
	
			<Enum name="RECURRENCE_TYPE_APPOINTMENT_C">
				<Element name="None" type="RECURRENCE_SUPPORT_FIELD_DOMESTICNONE" />
				<Element name="Daily" type="RECURRENCE_SUPPORT_FIELD_GSM_DAILY" />
				<Element name="Weekly" type="RECURRENCE_SUPPORT_FIELD_GSM_DAILY"/>
				<Element name="Monthly" type="RECURRENCE_SUPPORT_FIELD_GSM_MONTHLY" />
			</Enum>

			<Enum name="RECURRENCE_TYPE_APPOINTMENT_ANDROID">
        			<Element name="None" type="NONE" />
        			<Element name="Daily" type="NONE" />
        			<Element name="Workday" type="NONE" />
        			<Element name="Weekly" type="NONE"/>
        			<Element name="MonthlyNth" type="NONE" />
        			<Element name="Monthly" type="NONE" />
