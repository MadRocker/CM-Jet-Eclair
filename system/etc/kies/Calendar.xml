				<Element name="Item12"	 value="24" type="Hour"/>
				<Element name="Item13"	 value="2" type="Day"/>
				<Element name="Item14" value="1" type="Week"/>
			</Enum>
			
			<Enum Name="ALARM_TIME_LIST_TYPE_B">
				<Element Name="0" value="None"  type="None"/>
				<Element Name="1" value="0" type="OnTime"/>
				<Element Name="2" value="1" type="Day"/>
				<Element Name="3" value="3" type="Day"/>
				<Element Name="4" value="7" type="Day"/>
			</Enum>

			<Enum Name="ALARM_TIME_TYPE">
				<Element Name="None" value="0" type="None"/>
