            		<Title type="String" charset="Unicode" maxLength="50" />           
		        <StartDate type="DateTime" /> 
            		<EndDate type="DateTime" /> 
			<AllDay type="Boolean" /> 
	     		<Location type="String" charset="Unicode" maxLength="50" />                            
            		<Content type="String" charset="Unicode" maxLength="50" />
            		<Alarm >
				<AlarmTime type="ALARM_TIME_LIST_TYPE"/>  	          
			</Alarm>
			<Attendees type="String" charset="Unicode" maxLength="50" /> 
			<Recurrence type="RECURRENCE_TYPE_APPOINTMENT_ANDROID" /> 
	        </DataField>
		</Appointment>		
	</Schedule>
	
</Calendar>
</SDP>