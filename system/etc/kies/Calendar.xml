				<Element name="Business" Value="0"/>
				<Element name="Holiday" Value="1"/>
				<Element name="Personal" Value="2"/>
				<Element name="Seasonal" Value="3"/>
			</Enum>

			<Enum Name="ALARM_TIME_LIST_TYPE">
				<Element name="Item0" value="None" type="None"/>
